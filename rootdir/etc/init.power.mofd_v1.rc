on property:sys.boot_completed=1
    setprop sys.io.scheduler zen
    write /sys/block/mmcblk0/queue/read_ahead_kb 512

    write /sys/devices/system/cpu/cpu1/online 1
    write /sys/devices/system/cpu/cpu2/online 1
    write /sys/devices/system/cpu/cpu3/online 1
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor thessjactive
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor thessjactive
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor thessjactive
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor thessjactive
    write /sys/devices/system/cpu/cpufreq/above_hispeed_delay 0 1166000:40000 1416000:20000 1750000:0
    write /sys/devices/system/cpu/cpufreq/go_hispeed_load 95
    write /sys/devices/system/cpu/cpufreq/hispeed_freq 833000
    write /sys/devices/system/cpu/cpufreq/target_loads 0 250000:50 416000:35 750000:45 1083000:52 1166000:59 1333000:75 1416000:90 1650000:95 1833000:99
    write /sys/devices/system/cpu/cpufreq/timer_rate 20000
    write /sys/devices/system/cpu/cpufreq/timer_slack 20000
    write /sys/devices/system/cpu/cpufreq/touchboost_freq 1333000
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq ${ro.sys.perf.device.full}
    write /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq ${ro.sys.perf.device.full}
    write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq ${ro.sys.perf.device.full}
    write /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq ${ro.sys.perf.device.full}
    write /sys/devices/platform/dfrgx/devfreq/dfrgx/governor simple_ondemand
